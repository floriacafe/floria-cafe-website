{
  "version": 2,
  "rewrites": [
    { "source": "/Floria-pics/:path*", "destination": "/floria-pics/:path*" },
    { "source": "/FLORIA-pics/:path*", "destination": "/floria-pics/:path*" },

    { "source": "/Models/:path*", "destination": "/models/:path*" },
    { "source": "/Assets/:path*", "destination": "/assets/:path*" },
    { "source": "/ASSETS/:path*", "destination": "/assets/:path*" },

    {
      "source": "/:path*",
      "has": [{ "type": "header", "key": "accept", "value": "text/html" }],
      "missing": [{ "type": "file", "name": ":path*" }],
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(?:png|jpe?g|gif|svg|webp|ico|css|js|map|txt|json|webmanifest|woff2?|ttf|eot|mp4|webm|gltf|glb|obj|mtl|fbx|dae|3ds|stl|ply|drc|hdr|exr|ktx2|basis)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
